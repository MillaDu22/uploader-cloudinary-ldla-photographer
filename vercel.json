{
    "rewrites": [
      { "source": "/api/(.*)", "destination": "https://backend-uploader-cloudinary-ldla-photographer.vercel.app/api/$1" }
    ],
    "builds": [
      { "src": "package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  